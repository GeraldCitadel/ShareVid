{
    "version": 2,
    "builds": [
        {
            "src": "./index.html",
            "use": "@vercel/static-build"
        },
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        },
        {
            "src": "/api/(.*)",
            "dest": "./index.js"
        }
    ]
    
}